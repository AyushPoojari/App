<LoginScreen>:
    name:'LoginScreen'
    MDFloatLayout:
        md_bg_color:app.theme_cls.accent_color
        Image:
            source:'Logo.png'
            pos_hint:{"center_x":.5,"center_y":.9}
            allow_stretch:False
            keep_ratio:False

        MDLabel:
            text:'Login'
            pos_hint:{"center_x":.5,"center_y":.6}
            font_size:'40sp'
            font_name:'Cairo-Bold'
            color:(20/255.0,29/255.0,36/255.0,0.9)
            halign:'center'

    MDFloatLayout:
        size_hint: .85,.08
        pos_hint:{"center_x":.5,"center_y":.48}
        canvas:
            Color:
                rgb:(238/255,238/255,238/255,1)
            RoundedRectangle:
                size:self.size
                pos:self.pos
                radius:[25]
        TextInput:
            hint_text:'Email'
            size_hint:1,None
            pos_hint:{"center_x":.5,"center_y":.6}
            height:self.minimum_height
            font_name:'Cairo-Bold'
            multiline:False
            cursor_color:(20/255.0,29/255.0,36/255.0,0.9)
            cursor_width:"2sp"
            foreground_color:(20/255.0,29/255.0,36/255.0,0.9)
            background_color:0,0,0,0
            padding:15

    MDFloatLayout:
        size_hint: .85,.08
        pos_hint:{"center_x":.5,"center_y":.34}
        canvas:
            Color:
                rgb:(238/255,238/255,238/255,1)
            RoundedRectangle:
                size:self.size
                pos:self.pos
                radius:[25]
        TextInput:
            hint_text:'Password'
            size_hint:1,None
            pos_hint:{"center_x":.5,"center_y":.6}
            height:self.minimum_height
            multiline:False
            font_name:'Cairo-Bold'
            cursor_color:(20/255.0,29/255.0,36/255.0,0.9)
            cursor_width:"2sp"
            foreground_color:(20/255.0,29/255.0,36/255.0,0.9)
            background_color:0,0,0,0
            padding:15
            password:True

    MDFloatLayout:
        MDTextButton:
            text:"Forgot Your Password?"
            font_name:'Cairo-Bold'
            text_color:(20/255.0,29/255.0,36/255.0,0.9)
            pos_hint:{"center_x":.5,"center_y":.28}

        Button:
            text:"Login"
            font_name:"Cairo-Bold"
            font_size:"20sp"
            size_hint:.5,.08
            pos_hint:{"center_x":.5,"center_y":.18}
            background_color:0,0,0,0
            on_press:
                app.root.current = "HomeScreen"
                root.manager.transition.direction = "left"
            canvas.before:
                Color:
                    rgb:(20/255.0,29/255.0,36/255.0,0.9)
                RoundedRectangle:
                    size:self.size
                    pos:self.pos
                    radius:[23]
        MDLabel:
            text:'or'
            pos_hint:{"center_x":.5,"center_y":.12}
            font_size:'20sp'
            font_name:'Cairo-Bold'
            color:(20/255.0,29/255.0,36/255.0,0.9)
            halign:'center'

        Button:
            text:"Create Account"
            font_name:"Cairo-Bold"
            font_size:"20sp"
            size_hint:.5,.08
            pos_hint:{"center_x":.5,"center_y":.05}
            background_color:0,0,0,0
            on_press:
                app.root.current = "CreateScreen"
                root.manager.transition.direction = "left"
            canvas.before:

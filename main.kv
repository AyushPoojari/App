<HomeScreen>:
    name:'HomeScreen'
    MDBoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: "Data Structure Visualizer"
            font_name:'Cairo-Bold'
            font_size:'40sp'
            anchor_title: 'center'
            md_bg_color:(20/255.0,29/255.0,36/255.0,0.9)
            left_action_items: [["account-circle", lambda x: app.callback(x), "Ayush Poojari"]]
            right_action_items: [["dots-vertical", lambda x: app.callback(x), "Settings"]]

        MDSeparator:
            height:"4dp"
        Image:
            pos_hint:{"centre_x":0.5,"centre_y":0.5}
            allow_stretch:True
            keep_ratio:False
            size_hint:1,2
            source: 'DataStructure.png'
        MDSeparator:
            height:"2dp"

        MDFillRoundFlatButton:
            size_hint:1,.4
            text: "Explore"
            pos_hint:{"centre_x":0.5,"centre_y":0.5}
            font_name:'Cairo-Bold'
            font_size:"15dp"
            md_bg_color:(20/255.0,29/255.0,36/255.0,0.9)
            on_press:
                app.root.current = "DataStructureWindow"
                root.manager.transition.direction = "left"

        MDSeparator:
            height:"4dp"

        Image:
            pos_hint:{"centre_x":0.5,"centre_y":0.5}
            allow_stretch:True
            keep_ratio:False
            size_hint:1,2
            source: 'AS.png'
        MDSeparator:
            height:"2dp"

        MDFillRoundFlatButton:
            size_hint:1,.4
            text: "Explore"
            md_bg_color:(20/255.0,29/255.0,36/255.0,0.9)
            font_name:'Cairo-Bold'
            font_size:"15dp"
            on_press:
                app.root.current = "AlgorithmWindow"
                root.manager.transition.direction = "left"

        MDSeparator:
            height:"4dp"



<DataStructureWindow>:
    name:"DataStructureWindow"
    MDBoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: "DataStructure"
            font_name:'Cairo-Bold'
            font_size:"20dp"
            anchor_title: 'center'
            md_bg_color:(20/255.0,29/255.0,36/255.0,0.9)

        MDBoxLayout:
            orientation:'vertical'
            Button:
                size_hint:1,0.1
                text: "1. Stack"
                font_name:'Cairo-Bold'
                font_size:"15dp"
                background_color:(20/255.0,29/255.0,36/255.0,0.9)
                on_press:
                    app.root.current = 'StackWindow'
                    root.manager.transition.direction = 'left'


            Button:
                size_hint:1,0.1
                text: "2. Queue"
                font_name:'Cairo-Bold'
                font_size:"15dp"
                background_color:(20/255.0,29/255.0,36/255.0,0.9)
                text_color: .2,.2,.6, 1
                font_name:'Roboto-Bold'
                on_press:
                    app.root.current = 'QueueWindow'
                    root.manager.transition.direction = 'left'

            Button:
                size_hint:1,0.1
                font_name:'Cairo-Bold'
                font_size:"15dp"
                text: "3. Array"
                background_color:(20/255.0,29/255.0,36/255.0,0.9)
                text_color: .2,.2,.6, 1
                on_press:
                    app.root.current = 'ArrayWindow'
                    root.manager.transition.direction = 'left'


        MDSeparator:
            height:"10dp"

        MDRaisedButton:
            text:'Back'
            md_bg_color:(20/255.0,29/255.0,36/255.0,0.9)
            font_name:'Cairo-Bold'
            font_size:"15dp"
            pos_hint:{'center_x':0.5,'center_y':0.5}
            on_press:
                app.root.current = "HomeScreen"
                root.manager.transition.direction = "right"



